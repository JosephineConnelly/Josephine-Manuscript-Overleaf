  keywords = {ARABIDOPSIS,CIRCADIAN CLOCK,DAY-LENGTH,GROWTH HABIT,LOCUS-T,MAP-BASED CLONING,MATURITY,PHYTOCHROME-A,SEED MASS,TIME GENE},
