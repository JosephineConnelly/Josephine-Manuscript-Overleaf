  keywords = {BACILLUS-SUBTILIS,DROSOPHILA-MELANOGASTER,ESCHERICHIA-COLI,MAXIMUM-LIKELIHOOD,NATURAL-SELECTION,PHYLOGENETIC ANALYSIS,PROTEIN EXPRESSION,TRANSFER-RNA GENES,TRANSLATIONAL ACCURACY,USAGE BIAS}
}

