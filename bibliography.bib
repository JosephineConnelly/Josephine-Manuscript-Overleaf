  langid = {english},
  keywords = {assembly,DNA sequencing,Plant genetics},
  file = {/Users/josephinelovemachine/Zotero/storage/5VHV8AYV/Schmutz et al. - 2010 - Genome sequence of the palaeopolyploid soybean.pdf}
}

